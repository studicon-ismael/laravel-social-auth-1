{
    "name": "mad-web/laravel-social-auth",
    "type": "library",
    "description": "Easy social auth integration with a lot of available providers",
    "keywords": [
        "mad-web",
        "social",
        "auth",
        "laravel",
        "socialite",
        "login",
        "signin"
    ],
    "homepage": "https://github.com/mad-web/social-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Mad Web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "~5.5.0|~5.6.0",
        "illuminate/events": "~5.5.0|~5.6.0",
        "illuminate/database": "~5.5.0|~5.6.0",
        "illuminate/routing": "~5.5.0|~5.6.0",
        "illuminate/broadcasting": "~5.5.0|~5.6.0",
        "socialiteproviders/manager": "~3.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0.0",
        "phpunit/phpunit": "^6.5|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\SocialAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\SocialAuth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "MadWeb\\SocialAuth\\SocialAuthServiceProvider"
            ]
        }
    }
}
